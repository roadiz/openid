{
    "name": "roadiz/openid",
    "description": "Roadiz sub-package for handling OpenID authentication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ambroise Maupate",
            "email": "ambroise@roadiz.io"
        }
    ],
    "require": {
        "php": ">=7.4",
        "lcobucci/jwt": "^4.1",
        "roadiz/jwt": "~1.7.0 || dev-main",
        "roadiz/random": "~1.7.0 || dev-main",
        "roadiz/models": "~1.7.0 || dev-develop",
        "guzzlehttp/guzzle": ">=6.3",
        "pimple/pimple": ">=3.3.1",
        "symfony/http-foundation": ">=5.0.7",
        "symfony/security-core": "5.4.*",
        "symfony/security-http": "5.4.*",
        "symfony/security-csrf": "5.4.*",
        "psr/cache": ">=1.0.1",
        "jms/serializer": "^3.11",
        "symfony/event-dispatcher-contracts": "^1.1.9 || ^2.4.0",
        "codercat/jwk-to-pem": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.57",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "RZ\\Roadiz\\OpenId\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
